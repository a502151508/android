<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhouf.hello"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.zhouf.hello.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zhouf.hello.RateActivity"
            android:label="@string/title_activity_rate" >
        </activity>
        <activity
            android:name="com.zhouf.hello.RateConfigActivity"
            android:label="@string/title_activity_rate_config" >
        </activity>
        <activity
            android:name="com.zhouf.hello.AMainActivity"
            android:label="@string/title_activity_amain" >
        </activity>
    </application>

</manifest>
